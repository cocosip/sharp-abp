<Project Sdk="Microsoft.NET.Sdk">

	<Import Project="..\..\..\..\common.props" />
	<Import Project="..\..\..\..\configureawait.props" />

	<PropertyGroup>
		<TargetFrameworks>$(ProjectFrameworks)</TargetFrameworks>
		<AssemblyName>SharpAbp.Abp.TransformSecurityManagement.Domain.Shared</AssemblyName>
		<PackageId>SharpAbp.Abp.TransformSecurityManagement.Domain.Shared</PackageId>
		<AssetTargetFallback>$(AssetTargetFallback);portable-net45+win8+wp8+wpa81;</AssetTargetFallback>
		<GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
		<GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
		<GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
		<Description>SharpAbp TransformSecurityManagement Module</Description>
		<RootNamespace />
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Volo.Abp.Validation" />
		<PackageReference Include="Microsoft.Extensions.FileProviders.Embedded" />
	</ItemGroup>

	<ItemGroup>
		<EmbeddedResource Include="SharpAbp\Abp\TransformSecurityManagement\Localization\Resources\*.json" />
		<Content Remove="SharpAbp\Abp\TransformSecurityManagement\Localization\Resources\*.json" />
	</ItemGroup>

</Project>
