    <AssemblyName>SharpAbp.Abp.DbConnectionsManagement.Domain.Shared</AssemblyName>
    <PackageId>SharpAbp.Abp.DbConnectionsManagement.Domain.Shared</PackageId>
    <AssetTargetFallback>$(AssetTargetFallback);portable-net45+win8+wp8+wpa81;</AssetTargetFallback>
