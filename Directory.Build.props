<Project>
	<PropertyGroup>
		<!--Project Frameworks-->
		<ProjectFrameworks>netstandard2.0;netstandard2.1;$(NetCoreFx)</ProjectFrameworks>
		<!--Net Core Framework-->
		<NetCoreFx>net9.0</NetCoreFx>
		<!--Microsoft-->
		<MicrosoftPackageVersion>[9.0.8,)</MicrosoftPackageVersion>
		<!--EfCore-->
		<EfCorePackageVersion>[9.0.8,)</EfCorePackageVersion>
		<!--Volo ABP packages-->
		<VoloAbpPackageVersion>[9.3.2,10.0.0)</VoloAbpPackageVersion>
		<!--FreeSql package-->
		<FreeSqlPackageVersion>[3.5.213,)</FreeSqlPackageVersion>
		<!--Masstransit package-->
		<MassTransitPackageVersion>[8.5.2,)</MassTransitPackageVersion>
	</PropertyGroup>

	<PropertyGroup>
		<IsTestProject Condition="$(MSBuildProjectFullPath.Contains('test')) and ($(MSBuildProjectName.EndsWith('.Tests')) or $(MSBuildProjectName.EndsWith('.TestBase')))">true</IsTestProject>
	</PropertyGroup>

</Project>